{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "GitOpsAccess",
      "Effect": "Allow",
      "Action": ["s3:PutObject", "s3:PutObjectAcl", "s3:ListBucket"],
      "Resource": [
        "arn:PARTITION:s3:::BUCKET_NAME",
        "arn:PARTITION:s3:::BUCKET_NAME/*"
      ]
    }
  ]
}
